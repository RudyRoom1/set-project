{
  "Statement": [
    {
      "Sid": "SNS topic policy for S3 bucket",
      "Effect": "Allow",
      "Principal": {
        "Service": "s3.amazonaws.com"
      },
      "Action": "SNS:Publish",
      "Resource": "arn:aws:sns:eu-north-1:211125684549:ImageTopic",
      "Condition": {
        "StringEquals": {
          "AWS:SourceAccount": "211125684549"
        },
        "ArnLike": {
          "AWS:SourceArn": "arn:aws:s3:::image-project-bucket"
        }
      }
    }
  ]
}

